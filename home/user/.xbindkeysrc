#Volume: mute, up, down
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute
"amixer set Master playback 3-"
    m:0x0 + c:122
    XF86AudioLowerVolume
"amixer set Master playback 3+"
    m:0x0 + c:123
    XF86AudioRaiseVolume
#Wifi switch
"rfswitch-bnwr"
    m:0x0 + c:246
    XF86WLAN
#Media keys
"mpc prev"
    m:0x0 + c:173
    XF86AudioPrev
"mpc toggle"
    m:0x0 + c:172
    XF86AudioPlay
"mpc next"
    m:0x0 + c:171
    XF86AudioNext
#lock
"slock"
    m:0x40 + c:9
    Mod4 + Escape
#lock and suspend
"slock & sleep 1 && sudo pm-suspend"
    m:0x41 + c:9
    Shift+Mod4 + Escape
#open xsel-ed link in surf
"surf $(xsel)"
    m:0x40 + c:32
    Mod4 + o
#Start duckduckgo in surf
"surf http://duckduckgo.com"
    m:0x40 + c:42
    Mod4 + g
#Start or switch to firefox
"xdt-activate-bnwr firefox"
    m:0x40 + c:25
    Mod4 + w
#Touchpad toggling
"toggle-touchpad-bnwr"
    m:0x0 + c:220
    XF86WebCam
