#Volume: mute, up, down
"amixer set Master toggle ; set-status-line-bnwr"
    m:0x0 + c:121
    XF86AudioMute
"amixer set Master playback 3- ; set-status-line-bnwr"
    m:0x0 + c:122
    XF86AudioLowerVolume
"amixer set Master playback 3+ ; set-status-line-bnwr"
    m:0x0 + c:123
    XF86AudioRaiseVolume
"mpc volume -3"
    m:0x4 + c:122
    Control + XF86AudioLowerVolume
"mpc volume +3"
    m:0x4 + c:123
    Control + XF86AudioRaiseVolume
"mpc volume 0"
    m:0x4 + c:121
    Control + XF86AudioMute
#Wifi switch
"rfswitch-bnwr ; set-status-line-bnwr"
    m:0x0 + c:246
    XF86WLAN
#Media keys
"mpc prev"
    m:0x0 + c:173
    XF86AudioPrev
"mpc toggle"
    m:0x0 + c:172
    XF86AudioPlay
"mpc next"
    m:0x0 + c:171
    XF86AudioNext
#lock
"slock"
    m:0x40 + c:9
    Mod4 + Escape
"slock & sleep 1 && sudo pm-suspend"
    m:0x41 + c:9
    Shift+Mod4 + Escape
#plumb xsel-ed text
"plumb $(xsel)"
    m:0x40 + c:32
    Mod4 + o
#Start duckduckgo in surf
"surf http://duckduckgo.com"
    m:0x40 + c:42
    Mod4 + g
#Start firefox
"firefox -P socked"
    m:0x40 + c:25
    Mod4 + w
#Touchpad toggling
"toggle-touchpad-bnwr"
    m:0x0 + c:220
    XF86WebCam
#start useful programs
"st -e 9rc"
    m:0x40 + c:51
    Mod4 + backslash
"st"
    m:0x40 + c:36
    Mod4 + Return
"9acme"
    m:0x40 + c:26
    Mod4 + e
