echo -n ' ' \
  'Vol:' $(amixer get Master | grep "Mono:" | sed -e "s/.*\[\([0-9]*%\)\].*\[\(.*\)\]$/\2 \1/") '|' \
  'Wlan0:' $(netcfg current) '|' \
  $(acpi -a | sed 's/Adapter .: //; s/on-line/AC: /g; s/off-line/Bat :/g;') \
  $(acpi -b | sed 's/Battery .:.* \([0-9]*%\).*$/\1/') '|' \
  $(cpufreq-info -c 0 -p | awk '{print $3}') '|' \
  $(date '+%Y-%m-%d %a  %H:%M')
