set guioptions-=m
set guioptions-=T
set guioptions-=L
set guioptions-=r
set guioptions+=o
set guifont=Monospace\ 9
set lines=999
set columns=80
map <ScrollWheelUp> <C-Y>
map <ScrollWheelDown> <C-E>
map <ScrollWheelLeft> zh
map <ScrollWheelRight> zl

command Focus :set laststatus=0 |set guifont=Monospace\ 20
