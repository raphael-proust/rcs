echo -n ' Vol:' $(amixer get Master | grep "Front Left:" | sed -e "s/.*\[\([0-9]*%\)\].*/\1/") '|' 'Wlan0:' $(iwconfig wlan0 | awk '/Quality/{print $2}' | sed 's/.*=//' | awk -F"/" '{printf("%.0f%%", $1/$2*100)}') '| Bat:' $(acpi -b | sed -n 's/.*\([0-9][0-9]:[0-9][0-9]\):[0-9][0-9].*/\1/p') '|' $(cpufreq-info -c 0 -p | awk '{print $3}') ':' $(uptime | sed 's/.*: //; s/,//g' | awk '{print $1}') '|' $(date '+%a %Y-%m-%d  %H:%M')
