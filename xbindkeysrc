#Volume: mute, up, down
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute
"amixer set Master playback 3-"
    m:0x0 + c:122
    XF86AudioLowerVolume
"amixer set Master playback 3+"
    m:0x0 + c:123
    XF86AudioRaiseVolume
#Wifi switch
"rfswitch"
    m:0x0 + c:246
    XF86WLAN
#Media keys
"mpc prev"
    m:0x0 + c:173
    XF86AudioPrev
"mpc toggle"
    m:0x0 + c:172
    XF86AudioPlay
"mpc next"
    m:0x0 + c:171
    XF86AudioNext
#lock
"slock"
    m:0x40 + c:9
    Mod4 + Escape
